Extension { #name : #Halt }

{ #category : #'*TaggedHalts' }
Halt class >> disable: aHaltTag [
	TaggedHalt disable: aHaltTag
]

{ #category : #'*TaggedHalts' }
Halt class >> enable: aHaltTag [
	TaggedHalt enable: aHaltTag
]

{ #category : #'*TaggedHalts' }
Halt class >> resetTags [
	TaggedHalt resetTags
]
